<h1>All Tags</h1>

<ul id="tags">
   <%= @tags.each do |tag| %>
   <li>
      <%= link_to tag.name, tag_path(tag), class: 'tag_name' %> 
<% if logged_in? %>
<%= link_to "Delete", tag_path(tag), method: :delete, data: {confirm: "Really Nigga?"} %> 
<% end %>
   </li>
   <% end %>

<%= link_to "<< Back to Articles List", articles_path %>
</ul>
