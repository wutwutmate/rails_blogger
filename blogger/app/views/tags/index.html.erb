<h1>All Tags</h1>

<ul id="tags">
   <%= @tags.each do |tag| %>
   <li>
      <%= link_to tag.name, tag_path(tag), class: 'tag_name' %> <%= link_to "Delete", tag_path(tag), method: :delete, date: {confirm: "Really Nigga?"} %>
   </li>
   <% end %>

<%= link_to "<< Back to Articles List", articles_path %>
</ul>
